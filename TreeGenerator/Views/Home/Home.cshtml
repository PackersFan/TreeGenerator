@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Tree Generator</title>
</head>
<body>
    <div>
        Hello! This is a tree generator. 
    </div>
    <ul>
        @{ foreach (var root in ViewBag.Roots)
            {

                <li>@root.name</li>


            }
        }
    </ul>
</body>
</html>
